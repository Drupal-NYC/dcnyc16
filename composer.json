{
    "name": "drupaleurope/drupal_europe",
    "description": "Drupal Europe",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [ ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "bower-asset/dropzone": "^5.1",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/access_unpublished": "^1.0",
        "drupal/admin_toolbar": "^2.2",
        "drupal/adminimal_admin_toolbar": "^1.8",
        "drupal/adminimal_theme": "^1.3",
        "drupal/autocomplete_deluxe": "^1.0",
        "drupal/bulk_update_fields": "^1.0",
        "drupal/classy_paragraphs": "^1.0",
        "drupal/config_ignore": "^2.1",
        "drupal/config_selector": "^1.0",
        "drupal/config_update": "^1.5",
        "drupal/console": "^1.6",
        "drupal/core": "^8.8",
        "drupal/core-recommended": "8.8.4",
        "drupal/csv_serialization": "^1.3",
        "drupal/ctools": "^3.0",
        "drupal/default_content": "^1.0",
        "drupal/devel": "^2.1",
        "drupal/dropzonejs": "^2.0",
        "drupal/empty_fields": "^1.0",
        "drupal/entity": "^1.0",
        "drupal/entity_browser": "1.7",
        "drupal/entity_browser_enhanced": "^1.0",
        "drupal/eu_cookie_compliance": "^1.0",
        "drupal/field_group": "^1.0",
        "drupal/field_permissions": "^1.0",
        "drupal/flag": "^4.0",
        "drupal/honeypot": "^1.27",
        "drupal/inline_entity_form": "^1.0",
        "drupal/libraries": "^3.0",
        "drupal/linkit": "^4.3",
        "drupal/markup": "^1.0",
        "drupal/menu_block": "^1.5",
        "drupal/metatag": "^1.12",
        "drupal/paragraphs": "^1.3",
        "drupal/pathauto": "^1.1",
        "drupal/redirect": "^1.2",
        "drupal/restui": "^1.15",
        "drupal/scheduler": "^1.0",
        "drupal/shariff": "^1.3",
        "drupal/shield": "^1.2",
        "drupal/simple_sitemap": "^3.5",
        "drupal/smart_trim": "^1.1",
        "drupal/smtp": "^1.0",
        "drupal/social_media_links": "^2.6",
        "drupal/svg_image": "^1.8",
        "drupal/token": "^1.1",
        "drupal/viewsreference": "^2.0.0",
        "drupal/webform": "^5.0",
        "drush/drush": "^9.7.2",
        "oomphinc/composer-installers-extender": "^1.1",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/core-dev": "8.8.4",
        "zaporylie/composer-drupal-optimizations": "^1.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "enable-patching": true,
        "patches": {
            "drupal/diff": {
                "Back button for comparison page": "https://www.drupal.org/files/issues/back_button_for-2853193-4.patch"
            },
            "drupal/entity_browser": {
                "Provide inline entity form FieldWidgetDisplay": "https://www.drupal.org/files/issues/2858438_6.patch"
            }
        }
    }
}
